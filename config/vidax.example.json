{
  "apiKey": "set-me",
  "port": 3000,
  "bind": "127.0.0.1",
  "state_dir": "~/.va/state",
  "assets_config": "~/.va/state/config/assets.json",
  "comfyui": {
    "command": "python",
    "args": ["main.py", "--listen", "127.0.0.1", "--port", "8188"],
    "cwd": "../ComfyUI",
    "url": "http://127.0.0.1:8188",
    "health_endpoint": "/system_stats",
    "prompt_endpoint": "/prompt",
    "startup_timeout_ms": 60000,
    "poll_interval_ms": 500,
    "env": {},
    "auto_start": true,
    "paths": {
      "workflows_dir": "~/.va/state/comfyui/workflows",
      "models_dir": "~/.va/state/comfyui/models"
    }
  }
}
